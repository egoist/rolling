{
  "name": "rolling",
  "version": "0.0.3",
  "description": "A preset Rollup workflow.",
  "license": "MIT",
  "repository": "egoist/rolling",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "bin": "cli.js",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo index.js lib/*.js && ava test/test*.js"
  },
  "files": [
    "index.js",
    "cli.js",
    "lib"
  ],
  "keywords": [
    "rollup",
    "bundle",
    "webpack"
  ],
  "devDependencies": {
    "ava": "latest",
    "xo": "latest"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "operator-linebreak": [
        "error",
        "before"
      ]
    }
  },
  "dependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "cac": "^1.0.0",
    "co": "^4.6.0",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0"
  }
}
